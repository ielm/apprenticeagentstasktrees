@EXE.DECISION = {};
@EXE.EFFECTOR = {};
@EXE.CAPABILITY = {};
@EXE.CALLBACK = {};

@EXE.PHYSICAL-EFFECTOR = {
    ISA @EXE.EFFECTOR
};

@EXE.VERBAL-EFFECTOR = {
    ISA @EXE.EFFECTOR
};

@EXE.MENTAL-EFFECTOR = {
    ISA @EXE.EFFECTOR
};

@EXE.GET-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP  "get_physical_object";
};

@EXE.HOLD-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP  "hold_physical_object";
};

@EXE.FASTEN-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP "fasten_physical_object";
}

@EXE.RESTRAIN-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP "restrain_physical_object"
}

@EXE.LEARN-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP  "understand_input";
};

@EXE.SPEAK-CAPABILITY = {
    ISA @EXE.CAPABILITY;
    MP  "speak";
};

@SELF.PHYSICAL-EFFECTOR.1 = {
    ISA @EXE.PHYSICAL-EFFECTOR;
    HAS-CAPABILITY @EXE.GET-CAPABILITY;
    STATUS "FREE";
};

@SELF.MENTAL-EFFECTOR.1 = {
    ISA @EXE.MENTAL-EFFECTOR;
    HAS-CAPABILITY @EXE.LEARN-CAPABILITY;
    STATUS "FREE";
};

@SELF.VERBAL-EFFECTOR.1 = {
    ISA @EXE.VERBAL-EFFECTOR;
    HAS-CAPABILITY @EXE.SPEAK-CAPABILITY;
    STATUS "FREE";
};

@SELF.ROBOT.1 += {
    PRIORITY_WEIGHT 1.5;
    RESOURCES_WEIGHT 0.25;
    HAS-EFFECTOR @SELF.PHYSICAL-EFFECTOR.1;
    HAS-EFFECTOR @SELF.MENTAL-EFFECTOR.1;
    HAS-EFFECTOR @SELF.VERBAL-EFFECTOR.1;
};