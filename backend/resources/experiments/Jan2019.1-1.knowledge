    CONTAINS @ENV.SCREWDRIVER.1;
    CONTAINS @ENV.BRACKET.1;
    DISTANCE @ENV.SPATIAL-DISTANCE.1;
    DISTANCE @ENV.SPATIAL-DISTANCE.2;
